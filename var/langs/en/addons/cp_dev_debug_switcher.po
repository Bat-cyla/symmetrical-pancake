msgctxt "Addons::name::cp_dev_debug_switcher"
msgid "Cart-Power: Debug switcher [DEV HELPER]"
msgstr "Cart-Power: Debug switcher [DEV HELPER]"

msgctxt "Addons::description::cp_dev_debug_switcher"
msgid "Toggles debug mode and development mode with error display. Developed by <a target=\"_blank\" href=\"https://cart-power.com/\">Cart-Power</a>"
msgstr "Toggles debug mode and development mode with error display. Developed by <a target=\"_blank\" href=\"https://cart-power.com/\">Cart-Power</a>"

msgctxt "SettingsSections::cp_dev_debug_switcher::general"
msgid "General"
msgstr "General"

msgctxt "Languages::cp_dev_debug_switcher.enable_debug_mode"
msgid "Enable debug mode"
msgstr "Enable debug mode"

msgctxt "Languages::cp_dev_debug_switcher.enable_debug_mode_desc"
msgid "Activate <code>DEBUG_MODE</code>."
msgstr "Activate <code>DEBUG_MODE</code>."

msgctxt "Languages::cp_dev_debug_switcher.enable_debug_mode_desc_notice"
msgid "<code>DEBUG_MODE</code> is already defined outside of the add-on. Debug mode switching functionality is disabled. Check your settings in the config files."
msgstr "<code>DEBUG_MODE</code> is already defined outside of the add-on. Debug mode switching functionality is disabled. Check your settings in the config files."

msgctxt "Languages::cp_dev_debug_switcher.enable_development_mode"
msgid "Enable develop mode"
msgstr "Enable develop mode"

msgctxt "Languages::cp_dev_debug_switcher.enable_development_mode_desc"
msgid "Activate <code>DEVELOPMENT</code>, <code>display_errors</code>, <code>display_startup_errors</code>."
msgstr "Activate <code>DEVELOPMENT</code>, <code>display_errors</code>, <code>display_startup_errors</code>."

msgctxt "Languages::cp_dev_debug_switcher.enable_development_mode_notice"
msgid "<code>DEVELOPMENT</code> is already defined outside of the module. The development mode switching functionality is limited. Check your settings in the config files. <p>When switching setting, only <code>display_errors</code>, <code>display_startup_errors</code> will be activated.</p>"
msgstr "<code>DEVELOPMENT</code> is already defined outside of the module. The development mode switching functionality is limited. Check your settings in the config files. <p>When switching setting, only <code>display_errors</code>, <code>display_startup_errors</code> will be activated.</p>"

msgctxt "Languages::cp_dev_debug_switcher.enable_error_logging"
msgid "Enable error logging"
msgstr "Enable error logging"

msgctxt "Languages::cp_dev_debug_switcher.enable_error_logging_desc"
msgid "Enables logging errors, warnings and notices to a separate errors.log file in the module folder."
msgstr "Enables logging errors, warnings and notices to a separate errors.log file in the module folder."

